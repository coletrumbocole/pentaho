<?xml version="1.0" encoding="UTF-8"?>
<data-definition xmlns="http://reporting.pentaho.org/namespaces/engine/classic/bundle/data/1.0">
  <parameter-definition>
    <plain-parameter name="tenantUrl" mandatory="true" type="java.lang.String" default-value="jdbc:mysql://localhost:3306/mifostenant-default">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Tenant DB</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </plain-parameter>
    <plain-parameter name="username" mandatory="true" type="java.lang.String" default-value="root">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">User Name</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </plain-parameter>
    <plain-parameter name="password" mandatory="true" type="java.lang.String" default-value="mysql">
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="role">user</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="label">Password</attribute>
      <attribute namespace="http://reporting.pentaho.org/namespaces/engine/parameter-attributes/core" name="hidden">false</attribute>
    </plain-parameter>
  </parameter-definition>
  <data-source report-query="DRpentaho" limit="-1" ref="datasources/compound-ds.xml"/>
</data-definition>
